    <SolidColorBrush x:Key="Red" Color="#FF321E" />
    <SolidColorBrush x:Key="Orange" Color="#ff7a00" />
    <SolidColorBrush x:Key="DarkGray" Color="#969696" />
    <ImageBrush x:Key="BackgroundImg" ImageSource="/Images/LightBackground.png" />

