			<SolidColorBrush x:Key="Expander.Pressed.Arrow.Stroke" Color="{Binding Source={StaticResource Foreground1}}"/>
			<SolidColorBrush x:Key="Expander.Disabled.Circle.Stroke" Color="Transparent"/>
